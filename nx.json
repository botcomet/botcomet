{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test:ts"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build", "lint"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "test:ts": {
      "dependsOn": [
        "^test:ts", "build"
      ]
    }
  },
  "defaultBase": "master"
}
